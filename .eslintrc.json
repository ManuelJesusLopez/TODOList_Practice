{
  "env": {
    "browser": true,
    "es2020": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": 11,
    "sourceType": "module"
  },
  "rules": {
    "quotes": "off",
    "no-console": "warn",
    "arrow-body-style": "off",
    "max-len": "off",
    "operator-linebreak": "off"
  }
}
